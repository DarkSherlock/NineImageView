<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NineImageView">
        <attr name="ngv_singleImageWidth" format="dimension"/>
        <attr name="ngv_singleImageHeight" format="dimension"/>
        <attr name="ngv_gridSpacing" format="dimension"/>
        <attr name="ngv_maxSize" format="integer"/>
        <attr name="ngv_mode" format="enum">
            <enum name="fill" value="0"/>
            <enum name="grid" value="1"/>
        </attr>
    </declare-styleable>
</resources>